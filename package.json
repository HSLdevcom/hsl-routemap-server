{
  "name": "hsl-map-publisher",
  "version": "0.1.0",
  "description": "HSL Map Publisher",
  "main": "index.js",
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "prettier": "prettier \"src/**/*.{js,css}\" \"scripts/**/*.js\" \"migrations/**/*.js\" \"seeds/**/*.js\" \"*.js\" --write",
    "lint": "eslint \"src/**/*.js\" \"scripts/**/*.js\" --fix",
    "build": "webpack --config webpack.prod.js",
    "serve": "serve dist",
    "start": "webpack-serve ./webpack.dev.js",
    "server": "node scripts/server",
    "server:hot": "forever -w -t --killSignal=SIGTERM --watchDirectory scripts scripts/server.js",
    "knex": "PG_CONNECTION_STRING=postgres://postgres:postgres@localhost:5432/postgres knex",
    "docker:build": "docker build -t hsl-map-publisher .",
    "docker:run": "docker run -d -p 4000:4000 --name publisher -v $(pwd)/output:/output -v $(pwd)/fonts:/fonts --link publisher-postgres -e \"PG_CONNECTION_STRING=postgres://postgres:postgres@publisher-postgres:5432/postgres\" --shm-size=1G hsl-map-publisher",
    "docker:stop": "docker stop publisher",
    "docker:rm": "docker rm publisher",
    "docker:rebuild": "yarn run docker:stop && yarn run docker:rm && yarn run docker:build && yarn run docker:run",
    "docker:logs": "docker logs publisher"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HSLdevcom/hsl-map-publisher.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HSLdevcom/hsl-map-publisher/issues"
  },
  "homepage": "https://github.com/HSLdevcom/hsl-map-publisher#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.5.1",
    "forever": "^0.15.3",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "rimraf": "^2.6.2",
    "serve": "^6.4.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link-http": "^1.2.0",
    "classnames": "^2.2.5",
    "csv": "^2.0.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "hsl-map-style": "github:hsldevcom/hsl-map-style#21a85af2c4853e2c525d687d4ad297f16eb6d9dd",
    "iconv-lite": "^0.4.19",
    "knex": "^0.14.0",
    "koa": "^2.4.1",
    "koa-json-body": "^5.3.0",
    "koa-router": "7.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "node-fetch": "^1.7.3",
    "nodemon": "^1.12.1",
    "pg": "^7.4.0",
    "prop-types": "^15.6.0",
    "puppeteer": "^0.13.0",
    "qrcode": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.1.1",
    "recompose": "^0.26.0",
    "segseg": "^0.2.2",
    "uuid": "^3.1.0",
    "viewport-mercator-project": "^4.1.1",
    "worker-loader": "^1.1.0"
  }
}
